{
  "name": "zhaocnus-webpack-express-kickstart",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "nodemon server --exec 'npm run lint && node'",
    "lint": "eslint ./server",
    "build": "npm-run-all --sequential lint build:clean build:env build:client build:server",
    "build:env": "set NODE_ENV=production",
    "build:clean": "rimraf dist",
    "build:server": "babel ./server --out-dir dist/server",
    "build:client": "webpack --config ./webpack.prod.js --colors",
    "prod:serve": "NODE_ENV=production nodemon dist/server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.1",
    "compression": "^1.6.1",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "morgan": "^1.7.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "copy-webpack-plugin": "^2.1.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-loader": "^1.3.0",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.1",
    "image-webpack-loader": "^1.8.0",
    "ng-annotate-loader": "^0.1.0",
    "node-sass": "^3.7.0",
    "npm-run-all": "^1.8.0",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
